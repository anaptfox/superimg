{
  "name": "@superimg/types",
  "version": "0.1.0",
  "description": "TypeScript type definitions for SuperImg",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --outDir dist",
    "dev": "tsup src/index.ts --format esm --dts --outDir dist --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "superimg",
    "types"
  ],
  "license": "MIT",
  "dependencies": {
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@superimg/stdlib": "workspace:*"
  }
}
